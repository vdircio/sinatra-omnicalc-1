<form action="/random/results">
  <label for="minimum">Minimum</label>
  <input id="minimum" name="minimum" type="text" placeholder="E.g 1.5">
  <label for="maximum">Maximum</label>
  <input id="maximum" name="maximum" type="text" placeholder="E.g 4.5">
  <button type="submit">Pick random number</button>
</form>
